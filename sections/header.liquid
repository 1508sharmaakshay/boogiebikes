<style>
  .custom_down_arrow_wrapper {
    display: flex !important;
    align-items: center;
    gap: 0.5rem;
  }
  
  .custom_down_arrow_icon {
    width: 15px;
    height: 15px;
  }

  .custom_account_wrapper, .custom_cart_wrapper {
    display: flex;
    flex-direction: column;
    align-items: center;
  }

  .custom_cart_wrapper span {
    font-size: 14px !important;
  }

  .custom_account_wrapper, .custom_account_wrapper span {
    padding: 0 !important;
  }

  .custom_account_icon {
    width: 30px;
    height: 30px;
  }
  
  .custom_cart_icon {
    width:30px;
    height:30px;
  }

  .custom_nav_new {
    display: grid;
    grid-template-columns: 10% 40% 50%;
  }

  .custom_search_wrapper {
    display: flex;
    flex-direction: column;
    align-items: center;
  }

  .custom_search_icon, .custom_search_bar_icon {
    width:30px;
    height:30px;
  }

  .custom_search_label {
    font-family: Dosis !important;
    font-size: 20px;
    font-weight: bold;
    color: #132240;
    padding: 0 !important;
  }

  .custom_search_bar_input {
    font-size: 20px !important;
    height: auto !important;
    min-height: 0px !important;
  }

  .custom_search_bar_icon {
    position: absolute;
    right: 0%;
    top: 2%;
  }

  .custom_question_btn {
    padding: 2px 12px;
    width: 190px;
    border-radius: 0 !important;
  }
  
  .custom_get_bike_btn {
    padding: 12px;
    width: 190px;
    border-radius: 0 !important;
  }

  .custom_get_bike_btn a {
    font-size: 20px !important;
  }

  .custom_question_btn .custom_question_qstn {
    font-size: 12px !important; 
  }
  
  .custom_question_btn .custom_question_no {
    font-size: 17px !important;
  }

  .custom_nav_right, .custom_cart_desktop {
    margin-right: 40px !important;
    padding: 0 !important;
  }

  .custom_question_icon, .custom_question_qstn, .custom_question_no {
    display: none;
  }

  .custom_question_mobile {
    display: block !important;
    font-size: 20px !important;
    font-family: Dosis !important;
  }

  .custom_btn_wrapper {
    display: flex;
    gap: 20px;
  }

  @media screen and (max-width: 1347px) {
    .custom_get_bike_btn {
      width: 173px;
    }

    .custom_question_btn {
      width: 173px;
    }

    .custom_question_btn .custom_question_no {
      font-size: 15px !important;
    }
  }

  @media screen and (max-width: 1280px) {
    .custom_get_bike_btn, .custom_question_btn {
      width: 139px !important;
    }

    .custom_question_mobile, .custom_get_bike_btn a {
      font-size: 18px !important;
    }
  }
  
  @media screen and (max-width: 1210px) {
    .custom_question_btn {
      padding: 2px 5px;
    }
    
    .custom_get_bike_btn {
      padding: 5px;
    }

    .custom_get_bike_btn a {
      font-size: 18px !important;
    }

    .custom_question_btn .custom_question_no {
      font-size: 15px !important;
    }

    .custom_nav_right, .custom_cart_desktop {
      margin-right: 30px !important;
    }
  }

  @media screen and (max-width: 1127px) {
    .custom_question_btn .custom_question_no {
      font-size: 12px !important;
    }

    .custom_get_bike_btn a, .custom_question_mobile {
      font-size: 14px !important;
    }

    .custom_get_bike_btn, .custom_question_btn {
      width: 120px !important;
    }
  }

  @media screen and (max-width: 1056px) {
    .custom_question_mobile {
      display: block !important;
      font-size: 12px !important;
      font-family: Dosis !important;
    }

    .custom_question_qstn, .custom_question_no {
      display: none;
    }
    
    .custom_question_btn {
      width: 105px;
      padding: 2px 10px;
    }

    .custom_get_bike_btn {
      width: 105px;
    }

    .custom_mobile_logo {
      width: 42% !important;
      justify-content: flex-end !important;
      display: flex !important;
    }
  }

  @media screen and (max-width: 1024px) {
    .custom_get_bike_btn, .custom_question_btn {
      height: 48px !important;
      box-shadow: 2px 2px 3px rgb(4 4 4 / 50%);
    }

    .header {
      display: none;
    }

    #header {
      display: block;
    }

    .top-bar--left {
      height: 60px;
      display: flex;
      align-items: center;
    }

    .top-bar--right {
      height: 60px !important;
    }

    .custom_mobile_logo {
      position: relative !important;
      margin: 5px 10px !important;
    }

    .custom_mobile_logo  img {
      width: 100% !important;
      max-width: 75px !important;
      max-height: none !important;
    }

    .top-bar--right {
      height: 60px !important;
    }

    .top-bar--left {
      height: 60px;
      display: flex;
      align-items: center;
    }

    .custom_menu_wrapper {
      display: flex;
      flex-direction: column;
    }

    .custom_menu {
      width: 100% !important;
    }

    .menu_title {
      font-size: 14px !important;
    }

    .custom_cart_container {
      margin-right: 1rem;
    }

    .custom_dropdown {
      position: absolute !important;
      bottom: 0% !important;
    }

    .custom_cart_wrapper span {
      padding-bottom: 2.5px;
    }
  }

  @media screen and (max-width: 800px) {
    .custom_mobile_logo {
      width: 40.5% !important;
    }
  }

  @media screen and (max-width: 725px) {
    .custom_mobile_logo {
      width: 37% !important;
      justify-content: center !important;
    }
  }

  @media screen and (max-width: 640px) {
    .custom_mobile_logo {
      width: 28% !important;
    }
  }

  @media screen and (max-width: 554px) {
    .custom_question_icon {
      display: block;
      aspect-ratio: 1/1;
      width: 25px;
      height: 25px;
    }

    .custom_mobile_logo {
      width: 40% !important;
    }

    .custom_question_mobile {
      display: none !important;
    }

    .custom_question_btn {
      width: 48px !important;
    }

    .custom_btn_wrapper {
      gap: 5px;
    }
    
    .menu_title, .custom_get_bike_btn a, .custom_question_mobile, .custom_cart_wrapper span {
      font-size: 12px !important;
    }
  }

  @media screen and (max-width: 505px) {
    .custom_mobile_logo {
      width: 30% !important;
    }
  }

   @media screen and (max-width: 435px) {
    .custom_mobile_logo {
      width: 25% !important;
    }
  }

  @media screen and (max-width: 420px) {
    .custom_cart_wrapper span, .custom_question_mobile , .menu_title, .custom_get_bike_btn a  {
