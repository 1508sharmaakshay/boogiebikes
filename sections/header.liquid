    }

    .custom_cart_icon {
      width:25px;
      height:30px;
    }

    .custom_menu div {
      width: 20px !important;
    }

    .custom_cart_container {
      margin-right: 0.5rem;
    }

    .custom_mobile_logo {
      margin: 5px !important;
    }

    .custom_question_icon {
      width: 20px;
      height: 20px;
    }
  }

  @media screen and (max-width: 400px) {
    .custom_mobile_logo  img {
      max-width: 50px !important;
    }

